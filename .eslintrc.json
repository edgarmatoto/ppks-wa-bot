{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": "off",
        "linebreak-style": "off",
        "no-console": "off",
        "no-underscore-dangle":"off",
        "padded-blocks":"off"
    }
}
